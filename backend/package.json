{
  "name": "lead-scraper-backend",
  "version": "3.0.0",
  "description": "Lead Scraper PRO - Free Google Maps Scraper",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
